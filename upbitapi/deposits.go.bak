package upbitapi

import (
	"context"
)

type DepositsKrwBody struct {
	Amount        string `json:"amount"`
	TwoFactorType string `json:"two_factor_type"`
}

func DepositsKrw(ctx context.Context) (*MarketInfos, error) {
	var accountInfos MarketInfos
	accountInfos, err := commonRestGet(ctx, depositsKrwEndPoint, accountInfos)
	if err != nil {
		return nil, err
	}
	return &accountInfos, nil
}
